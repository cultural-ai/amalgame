@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix amalgame: <http://purl.org/vocabularies/amalgame#> .

amalgame:ExactLabelMatcher
    rdfs:label "exact label matcher"@en ;
    skos:definition "A basic label matcher"@en ;
    rdfs:subClassOf amalgame:Matcher .
amalgame:ExactLabelFilter
    rdfs:label "exact label filter"@en ;
    skos:definition "A filter of mappings based on exact matching labels of source and target concepts"@en ;
    rdfs:subClassOf amalgame:MatchFilter .

amalgame:SnowballMatcher
    rdfs:label "snowball matcher"@en ;
    skos:definition "A label matcher with similarity based on stemming."@en ;
    rdfs:subClassOf amalgame:Matcher .
amalgame:SnowballFilter
    rdfs:label "snowball filter"@en ;
    skos:definition "A filter of mappings based on the labels of source and target concept that match by stem"@en ;
    rdfs:subClassOf amalgame:MatchFilter .

amalgame:IsubMatcher        
	rdfs:label "isub string matcher"@en ;
	skos:definition "An isub string similarity matcher based on the metric described by 'A string metric for ontology alignment' by Giorgos Stoilos, 2005."@en ;
	rdfs:subClassOf amalgame:Matcher .
amalgame:IsubFilter
    rdfs:label "Isub filter"@en ;
    skos:definition "A filter of mappings based on string similarity metric described by 'A string metric for ontology alignment' by Giorgos Stoilos, 2005."@en ;
    rdfs:subClassOf amalgame:MatchFilter .

amalgame:SelectN1
    rdfs:label "select unique target (n-1);"@en ;
    skos:definition "Select correspondences for which each source is mapped to only 1 target concept."@en ;
    rdfs:subClassOf amalgame:MappingSelecter .

amalgame:Select1N
    rdfs:label "select unique source (1-n);"@en ;
    skos:definition "Select correspondences for which each source is mapped to only 1 target concept."@en ;
    rdfs:subClassOf amalgame:MappingSelecter .

amalgame:Select11
    rdfs:label "select 1-1"@en ;
    skos:definition "Select correspondences that have only 1 source and 1 target concept."@en ;
    rdfs:subClassOf amalgame:MappingSelecter .
    
amalgame:MapMerger
    rdfs:label "mapping merger"@en ;
    skos:definition "Merge two or more mappings into one new mapping. This can, for example, to collect the good mappings made by different processes."@en ;
    rdfs:subClassOf amalgame:Merger .
