@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix doap:    <http://usefulinc.com/ns/doap#> .
@prefix opmv:    <http://purl.org/net/opmv/ns#> .
@prefix common:    <http://purl.org/net/opmv/types/common#> .
@prefix sparql:    <http://purl.org/net/opmv/types/sparql#> .
@prefix :    <http://purl.org/vocabularies/amalgame#> .
@prefix  ahm: <http://purl.org/collections/nl/am/> .


# Named toplevel resources (28,118)
<>  a :Workflow ;
    rdfs:label "AM-persons" .

_:exact_mapping_dbpedia
    a :Mapping ;
    rdfs:label "label match (exact)" ;
    opmv:wasGeneratedBy [
        a :Exact_Label_Matcher ;
        rdfs:label "Pref/Alt to Pref Label matching" ;
        :source <http://purl.org/collections/nl/am/AM_PersonScheme> ;
        :target <http://dbpedia.org> ;
        opmv:wasPerformedBy :Amalgame ;
    ] .	

_:exact_mapping_ulan
    a :Mapping ;
    rdfs:label "label match (exact)" ;
    opmv:wasGeneratedBy [
        a :Exact_Label_Matcher ;
        rdfs:label "Pref/Alt Label matching" ;
        :source	<http://purl.org/collections/nl/am/AM_PersonScheme> ;
        :target	<http://e-culture.multimedian.nl/ns/getty/ulan> ;
        opmv:wasPerformedBy :Amalgame ;
    ] .		
<http://e-culture.multimedian.nl/ns/getty/ulan> rdfs:label "Getty ULAN".	
<http://dbpedia.org> rdfs:label "DbPedia (Persons only)".


_:ambiguous_selection_ulan
    a :Select1_1 ;
    rdfs:label "Ambiguity selection" ;
    :input _:exact_mapping_ulan ;
    opmv:wasPerformedBy :Amalgame .

_:select11
    a :Mapping ;
    rdfs:label "Unambiguous mapping (1-1)" ;
    :selectedBy _:ambiguous_selection_ulan .

_:select1N
    a :Mapping ;
    rdfs:label "Ambiguous mapping (1-N)" ;
    :discardedBy _:ambiguous_selection_ulan .

_:exclude_mapped a :Voc_Exclude;
    rdfs:label "Exclude mapped concepts" ;
    :source <http://purl.org/collections/nl/am/AM_PersonScheme> ;
    :target <http://dbpedia.org>;
    :parameters "type=source" ;	   
    :exclude _:exact_mapping_dbpedia.

_:tobematched a :Mapping ;
    rdfs:label "Not yet mapped" ;
    opmv:wasGeneratedBy _:exclude_mapped.
	
_:labelmatcherpapa a :Exact_Label_Matcher ;
    rdfs:label "Pref/Alt to Pref/Alt Label matching" ;
    :input _:tobematched  .

_:matchedpapa a :Mapping ;
    rdfs:label "label match (exact)" ;
    opmv:wasGeneratedBy _:labelmatcherpapa.
		

_:ambiguous_selection_dbp
    a :Select1_1 ;
    rdfs:label "Ambiguity selection" ;
    :input _:matchedpapa ;
    opmv:wasPerformedBy :Amalgame .

_:select11dbp
    a :Mapping ;
    rdfs:label "Unambiguous mapping (1-1)" ;
    :selectedBy _:ambiguous_selection_dbp .

_:select1Ndbp
    a :Mapping ;
    rdfs:label "Ambiguous mapping (1-N)" ;
    :discardedBy _:ambiguous_selection_dbp .	
	
	
	
	
	
	
	
	
	

	