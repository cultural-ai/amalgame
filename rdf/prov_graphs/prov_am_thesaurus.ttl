@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix doap:    <http://usefulinc.com/ns/doap#> .
@prefix opmv:    <http://purl.org/net/opmv/ns#> .
@prefix common:    <http://purl.org/net/opmv/types/common#> .
@prefix sparql:    <http://purl.org/net/opmv/types/sparql#> .
@prefix :    <http://purl.org/vocabularies/amalgame#> .
@prefix  ahm: <http://purl.org/collections/nl/am/> .


# Named toplevel resources (28,118)
<>  a :Workflow ;
    rdfs:label "AM-thesaurus" .

_:select_geo
	a :Voc_Exclude ;
	rdfs:label "Select on Property Values" ;
    :input	ahm:AM_ConceptScheme ;
    :parameters "property=termType, values=[GEOKEYW, PLACE, ...]" ;
	opmv:wasPerformedBy :Amalgame .

_:selectedG
    a :Mapping ;
    rdfs:label "AM Geographic" ;
    :selectedBy _:select_geo .

_:selectedNG
    a :Mapping;
    rdfs:label "AM Non-geographic" ;
    :discardedBy _:select_geo .
	
_:select_geo_nl
	a :Voc_Exclude ;
	rdfs:label "Select on subtree" ;
    :input	_:selectedG ;
    :parameters "property=skos:broader, values=[Netherlands]" ;
	opmv:wasPerformedBy :Amalgame .

_:selectedNL
    a :Mapping ;
    rdfs:label "AM Dutch Geo-concepts" ;
    :selectedBy _:select_geo_nl .

_:selectedNonNL
    a :Mapping;
    rdfs:label "AM Non-Dutch Geo-concepts" ;
    :discardedBy _:select_geo_nl .
	
_:select_geonames_nl
	a :Voc_Exclude ;
	rdfs:label "Select on subtree" ;
    :input	<http://sws.geonames.org/> ;
    :parameters "property=skos:broader, values=[Netherlands]" ;
	opmv:wasPerformedBy :Amalgame .
		
_:geoNamesSelected 
	a :Mapping ;
    rdfs:label "GeoNames NL" ;
    :selectedBy _:select_geonames_nl .	

_:exact_mapping_geo
    a :Mapping ;
    rdfs:label "label match (exact)" ;
    opmv:wasGeneratedBy [
        a :Exact_Label_Matcher ;
        rdfs:label "Pref/Alt Label matching" ;
        :source _:selectedNL ;
        :target _:geoNamesSelected ;
        opmv:wasPerformedBy :Amalgame ;
    ] .

<http://sws.geonames.org/> rdfs:label "GeoNames" .

_:exact_mapping_nongeo
    a :Mapping ;
    rdfs:label "label match (exact)" ;
    opmv:wasGeneratedBy [
        a :Exact_Label_Matcher ;
        rdfs:label "Pref/Alt Label matching" ;
        :source _:selectedNG ;
        :target <http://purl.org/vocabularies/rkd/aatned/aatned> ;
        opmv:wasPerformedBy :Amalgame ;
    ] .


_:ambiguous_selection
    a :Select1_1 ;
    rdfs:label "Ambiguity selection" ;
    :input _:exact_mapping_nongeo ;
    opmv:wasPerformedBy :Amalgame .

_:select11
    a :Mapping ;
    rdfs:label "Unambiguous mapping (1-1)" ;
    :selectedBy _:ambiguous_selection .

_:select1N
    a :Mapping ;
    rdfs:label "Ambiguous mapping (1-N)" ;
    :discardedBy _:ambiguous_selection .












	