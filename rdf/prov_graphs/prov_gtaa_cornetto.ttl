@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix doap:    <http://usefulinc.com/ns/doap#> .
@prefix opmv:    <http://purl.org/net/opmv/ns#> .
@prefix common:    <http://purl.org/net/opmv/types/common#> .
@prefix sparql:    <http://purl.org/net/opmv/types/sparql#> .
@prefix :    <http://purl.org/vocabularies/amalgame#> .

<>  a :Workflow ;
    rdfs:label "GTAA-Cornetto" .

<http://purl.org/vocabularies/cornetto> rdfs:label "Cornetto".
<http://data.beeldengeluid.nl/gtaa/Onderwerpen> rdfs:label "GTAA onderwerpen".
	
_:cornetto_split
	a :Voc_Exclude ;
	rdfs:label "Split on type" ;
    :input	<http://purl.org/vocabularies/cornetto>  ;
    :parameters "property=type" ;
	opmv:wasPerformedBy :Amalgame .

_:cornouns
    a :Mapping ;
    rdfs:label "Cornetto nouns" ;
    :selectedBy _:cornetto_split .

_:corverbs
    a :Mapping;
    rdfs:label "Cornetto verbs" ;
    :discardedBy _:cornetto_split .
	
_:noun_mapping
    a :Mapping ;
    rdfs:label "label match (stemming)" ;
    opmv:wasGeneratedBy [
        a :Snowball_Matcher ;
        rdfs:label "Noun mapping" ;
        :source _:cornouns ;
        :target <http://data.beeldengeluid.nl/gtaa/Onderwerpen>;
        :parameters "snowball_language=dutch" ;
        opmv:wasPerformedBy :Amalgame ;
    ] .

	
_:gtaa_subj_unmatched
	a :Voc_Exclude ;
	rdfs:label "Filter matched" ;
    :input	_:noun_mapping  ;
	opmv:wasPerformedBy :Amalgame .
	
_:gtaa_subj_unmatched_result
    a :Mapping ;
    rdfs:label "unmatched subjects" ;
    :input _:gtaa_subj_unmatched ;
    opmv:wasPerformedBy :Amalgame .	
	
_:cornetto_verb_mapping
    a :Mapping ;
    rdfs:label "label match (exact)" ;
    opmv:wasGeneratedBy [
        a :Exact_Label_Matcher ;
        rdfs:label "Verb mapping" ;
        :source _:corverbs ;
        :target _:gtaa_subj_unmatched_result ;
        opmv:wasPerformedBy :Amalgame ;
    ] .

_:noun_selection
    a :Select1_1 ;
    rdfs:label "Noun 1-1 selection" ;
    :input _:noun_mapping ;
    opmv:wasPerformedBy :Amalgame .

_:select11
    a :Mapping ;
    rdfs:label "Noun mapping 1-1" ;
    :selectedBy _:noun_selection .

_:select1N
    a :Mapping ;
    rdfs:label "Noun mapping 1-N" ;
    :discardedBy _:noun_selection .



_:filter_generic
    a :Select1_1 ;
    rdfs:label "Filter most generic" ;
    :input _:select1N ;
    opmv:wasPerformedBy :Amalgame .

_:filter_genS
    a :Mapping ;
    rdfs:label "Most Generic selected" ;
    :selectedBy _:filter_generic .

_:filter_genNS
    a :Mapping ;
    rdfs:label "undecided nouns" ;
    :discardedBy _:filter_generic .


	
	
_:tars
    a :Select1_1 ;
    rdfs:label "Filter target siblings" ;
    :input _:filter_genNS ;
    opmv:wasPerformedBy :Amalgame .

_:tarsS
    a :Mapping ;
    rdfs:label "Selected noun siblings" ;
    :selectedBy _:tars .

_:tarsNS
    a :Mapping ;
    rdfs:label "undecided nouns" ;
    :discardedBy _:tars .

	
	
_:hiersim
    a :Select1_1 ;
    rdfs:label "Filter hierarchy similarity" ;
    :input _:tarsNS ;
    opmv:wasPerformedBy :Amalgame .

_:hiersimS
    a :Mapping ;
    rdfs:label "Selected noun hierarchy similarity" ;
    :selectedBy _:hiersim .

_:hiersimNS
    a :Mapping ;
    rdfs:label "Undecided nouns" ;
    :discardedBy _:hiersim .	
	
	



