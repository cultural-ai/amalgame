@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix doap:    <http://usefulinc.com/ns/doap#> .
@prefix opmv:    <http://purl.org/net/opmv/ns#> .
@prefix common:  <http://purl.org/net/opmv/types/common#> .
@prefix sparql:  <http://purl.org/net/opmv/types/sparql#> .
@prefix exp: 	 <http://localhost/my/wn3020/> .
@prefix :   	 <http://purl.org/vocabularies/amalgame#> .

# Final results as discussed in the paper:

# Set 1a: 1-1 gloss matches
exp:gloss11 a :Mapping ;
    rdfs:label "F1a: Gloss mapping 1-1" ;
    :status :final ;
    :selectedBy exp:glossselect11process .

# Set 1b: 1-1 gloss + label matches
exp:glosslabel11 a :Mapping ;
    rdfs:label "F1b: Gloss label mapping 1-1" ;
    :status :final ;
    :selectedBy exp:glosslabelselect11process .

# Set 1c: 1-n gloss + label matches
exp:glosslabelnm a :Mapping ;
    rdfs:label "F1c: Gloss mapping N-M" ;
    :status :final ;
    :discardedBy exp:glosslabelselect11process .

# Set 2a: 1-1 nogloss but label matches
exp:label11 a :Mapping ;
    rdfs:label "F2a: Label mapping 1-1" ;
    :status :final ;
    :selectedBy exp:labelselect11process .

exp:tar_bestgloss a :Mapping ;
    rdfs:label "F2c: best targets from best near gloss matched sources" ;
    :status :final ;
    :selectedBy exp:tar_bestnearglossfilter.

exp:mostmethods11 a :Mapping ;
    rdfs:label "F4: one-to-one label matches disambiguated by selecting sources with most matching methods" ;
    :status :final ;
    :selectedBy exp:mostmarityselecter.


# Matching processes used to generate the (intermediate) results:
exp:glossmatcher a :ExactLabelMatcher ;
    rdfs:label "P1a: Exact gloss match process" ;
    :source <http://purl.org/vocabularies/princeton/wn30/> ;
    :target <http://www.w3.org/2006/03/wn/wn20/> ;
    :parameters "matchacross_type=false&sourcelabel=http://www.w3.org/2004/02/skos/core%23definition&targetlabel=http://www.w3.org/2004/02/skos/core%23definition" .

exp:glosslabelfilter a :ExactLabelMatcher ;
    rdfs:label "P1b: Exact label filter to disambiguate N to M gloss matches" ;
    :input exp:gloss1N ;
    :parameters "sourcelabel=http://www.w3.org/2004/02/skos/core%23altLabel&targetlabel=http://www.w3.org/2004/02/skos/core%23altLabel" .

exp:labelfilter a :ExactLabelMatcher ;
    rdfs:label "P2a: Exact label filter on non-gloss matches" ;
    :source exp:sourceexcluded ;
    :target exp:targetexcluded ;
    :parameters "sourcelabel=http://www.w3.org/2004/02/skos/core%23altLabel&targetlabel=http://www.w3.org/2004/02/skos/core%23altLabel" .

exp:nearglossmatcher a :IsubMatcher ;
    rdfs:label "P2b: Near label match process on non-gloss matches" ;
    :input exp:labelnm ;
    :parameters "sourcelabel=http://www.w3.org/2004/02/skos/core%23definition&targetlabel=http://www.w3.org/2004/02/skos/core%23definition" .

exp:ancmatcher a :AncestorMatcher ;
    rdfs:label "P3a: ancestor match to disambiguate near gloss matches" ;
    :parameters "steps=2" ;
    :input exp:ambigloss .

exp:descmatcher a :DescendentMatcher ;
    rdfs:label "P3a: descendent match to disambiguate near gloss matches" ;
    :parameters "steps=2" ;
    :input exp:ambigloss .

# Selector processes used to generate the (intermediate) results:
exp:glossselect11process a :AritySelect ;
    rdfs:label "S1a: 1-1 selection process" ;
    :input exp:gloss_mapping ;
    :parameters "type=11" .

exp:glosslabelselect11process a :AritySelect ;
    rdfs:label "S1b: 1-1 selection process" ;
    :input exp:gloss_label_mapping ;
    :parameters "type=11" .


exp:goodglosslabelsourceexcludeprocess a :VocExclude;
    rdfs:label "E1: Exclude good gloss and label matches from source" ;
    :input <http://purl.org/vocabularies/princeton/wn30/> ;
    :exclude exp:gloss11 ;
    :exclude exp:glosslabel11 ;
    :exclude exp:glosslabelnm ;
    :parameters "type=source" .
    
exp:goodglosslabeltargetexcludeprocess a :VocExclude;
    rdfs:label "E1: Exclude good gloss and label matches from target" ;
    :input <http://www.w3.org/2006/03/wn/wn20/> ;
    :exclude exp:gloss11 ;
    :exclude exp:glosslabel11 ;
    :exclude exp:glosslabelnm ;
    :parameters "type=target" .
    
exp:labelselect11process a :AritySelect ;
    rdfs:label "S2a: 1-1 selection process" ;
    :input exp:label_mapping ;
    :parameters "type=11" .

exp:mostmarityselecter a :AritySelect ;
    rdfs:label "S4b: 1-1 selection process" ;
    :input exp:mostmethods ;
    :parameters "type=11" .

exp:src_bestnearglossfilter a :BestNumeric ;
    rdfs:label "S2c select best sources from near gloss matches" ;
    :parameters "type=source" ;	   
    :input exp:glossnear.

exp:tar_bestnearglossfilter a :BestNumeric ;
    rdfs:label "S2c select best targets from near gloss matches" ;
    :parameters "type=target" ;	   
    :input exp:src_bestneargloss.

exp:mostmethodsfilter a :MostMethods ;
    rdfs:label "S4a select sources with most matching methods" ;
    :input exp:ambigloss2 .

exp:ambiglossmerger a :MapMerger;
    rdfs:label "M3: ambiguous near gloss matches merger" ;
    :input exp:src_ambigloss ;
    :input exp:tar_ambigloss .

exp:ambiglossmerger2 a :MapMerger;
    rdfs:label "M4: 2nd ambiguous near gloss matches merger" ;
    :input exp:src_ambigloss ;
    :input exp:tar_ambigloss ;
    :input exp:ancestormatches ;
    :input exp:descendentmatches.

# Intermediate mappings
# Intermediate mappings
exp:gloss_mapping a :Mapping ;
    rdfs:label "I1aa: Gloss mapping" ;
    opmv:wasGeneratedBy exp:glossmatcher.

exp:gloss_label_mapping a :Mapping ;
    rdfs:label "I1b: Gloss+label mapping" ;
    opmv:wasGeneratedBy exp:glosslabelfilter.

exp:gloss1N a :Mapping ;
    rdfs:label "I1ab: Gloss mapping N-M" ;
    :discardedBy exp:glossselect11process .

#exp:goodglosslabelmatches a :Mapping ;
#    rdfs:label "I2aa: Merge of all gloss-based labels we assume correct" ;
#    opmv:wasGeneratedBy exp:goodglosslabelmergeprocess.

#exp:tobematched a :Mapping ;
#    rdfs:label "I2ab: templates with still to be mapped source concepts, ignoring gloss-based labels we assume correct" ;
#    opmv:wasGeneratedBy exp:goodglosslabelsourceexcludeprocess.

exp:sourceexcluded a skos:ConceptScheme ;
    rdfs:label "I2ab: source concepts, ignoring gloss-based labels we assume correct" ;
    opmv:wasGeneratedBy exp:goodglosslabelsourceexcludeprocess.

exp:targetexcluded a skos:ConceptScheme ;
    rdfs:label "I2ab: target concepts, ignoring gloss-based labels we assume correct" ;
    opmv:wasGeneratedBy exp:goodglosslabeltargetexcludeprocess.

#

exp:label_mapping a :Mapping ;
    rdfs:label "I2ac: Label mapping of concepts we failed to match in step 1" ;
    opmv:wasGeneratedBy exp:labelfilter.

exp:labelnm a :Mapping ;
    rdfs:label "I2ba: Label mapping N-M" ;
    :discardedBy exp:labelselect11process .

exp:glossnear a :Mapping ;
    rdfs:label "I2c: Isub gloss match" ;
    opmv:wasGeneratedBy exp:nearglossmatcher.

exp:src_bestneargloss a :Mapping ;
    rdfs:label "I2c1: best sources from near gloss matches" ;
    :selectedBy exp:src_bestnearglossfilter.

exp:src_ambigloss a :Mapping ;
    rdfs:label "I2c2: ambiguous sources from near gloss matches" ;
    :undecidedBy exp:src_bestnearglossfilter.

exp:tar_ambigloss a :Mapping ;
    rdfs:label "I2c3: ambiguous targets from near gloss matches" ;
    :undecidedBy exp:tar_bestnearglossfilter.

exp:ambigloss a :Mapping ;
    rdfs:label "I3: ambiguous source and target matches merged" ;
    opmv:wasGeneratedBy exp:ambiglossmerger.
    
exp:ambigloss2 a :Mapping ;
    rdfs:label "I4a: ambiguous source and target matches merged" ;
    opmv:wasGeneratedBy exp:ambiglossmerger2.
    
exp:mostmethods a :Mapping ;
    rdfs:label "I4b: sources disamb by most methods" ;
    :selectedBy exp:mostmethodsfilter.
    
exp:ancestormatches a :Mapping ;
    rdfs:label "I3a: near matches disambiguated with ancestor matches" ;
    opmv:wasGeneratedBy exp:ancmatcher.
exp:descendentmatches a :Mapping ;
    rdfs:label "I3b: near matches disambiguated with descendentmatches" ;
    opmv:wasGeneratedBy exp:descmatcher.
    
exp:wn3020 a :Alignment ;
