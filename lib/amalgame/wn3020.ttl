@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix doap:    <http://usefulinc.com/ns/doap#> .
@prefix opmv:    <http://purl.org/net/opmv/ns#> .
@prefix common:    <http://purl.org/net/opmv/types/common#> .
@prefix sparql:    <http://purl.org/net/opmv/types/sparql#> .
@prefix :    <http://purl.org/vocabularies/amalgame#> .

# Final results as discussed in the paper:

# Set 1a: 1-1 gloss matches
_:gloss11 a :Mapping ;
    rdfs:label "Gloss mapping 1-1" ;
    :selected _:gloss_mapping ;
    opmv:wasGeneratedBy _:glossselect11process .

# Set 1b: 1-1 gloss + label matches
_:glosslabel11 a :Mapping ;
    rdfs:label "Gloss label mapping 1-1" ;
    :selected _:gloss_label_mapping ;
    opmv:wasGeneratedBy _:glosslabelselect11process .

# Set 1c: 1-n gloss + label matches
_:glosslabel11 a :Mapping ;
    rdfs:label "Gloss label mapping 1-1" ;
    :discarded _:gloss_label_mapping ;
    opmv:wasGeneratedBy _:glosslabelselect11process .

# Set 1c: 1-n gloss + label matches
_:glosslabel11 a :Mapping ;
    rdfs:label "Gloss label mapping 1-1" ;
    :discarded _:gloss_label_mapping ;
    opmv:wasGeneratedBy _:glosslabelselect11process .

# Matching processes used to generate the (intermediate) results:
_:glosslabelmatcher a :Exact_Label_Match ;
    rdfs:label "Exact label match process to disamb. 1N gloss matches" ;
    :input _:gloss1N ;
    :parameters "sourcelabel=http://www.w3.org/2004/02/skos/core%23altLabel&targetlabel=http://www.w3.org/2004/02/skos/core%23altLabel" ;
    opmv:wasPerformedBy :Amalgame .

_:glossmatcher a :Exact_Label_Match ;
    rdfs:label "Exact gloss match process" ;
    :source <http://purl.org/vocabularies/princeton/wn30/> ;
    :target <http://www.w3.org/2006/03/wn/wn20/> ;
    :parameters "sourcelabel=http://www.w3.org/2004/02/skos/core%23definition&targetlabel=http://www.w3.org/2004/02/skos/core%23definition" ;
    opmv:wasPerformedBy :Amalgame .



# Selector processes used to generate the (intermediate) results:
_:glossselect11process a :Arity_select ;
    rdfs:label "1-1 selection process" ;
    :input _:gloss_mapping ;
    opmv:wasPerformedBy :Amalgame .

_:glosslabelselect11process a :Arity_select ;
    rdfs:label "1-1 selection process" ;
    :input _:gloss_label_mapping ;
    opmv:wasPerformedBy :Amalgame .


# Intermediate mapping
_:gloss_mapping a :Mapping ;
    rdfs:label "Gloss mapping" ;
    opmv:wasGeneratedBy _:glossmatcher.

_:gloss_label_mapping a :Mapping ;
    rdfs:label "Gloss+label mapping" ;
    opmv:wasGeneratedBy _:glosslabelmatcher.

_:gloss1N a :Mapping ;
    rdfs:label "Gloss mapping 1-N" ;
    :discarded _:gloss_mapping ;
    opmv:wasGeneratedBy _:glossselect11process .

